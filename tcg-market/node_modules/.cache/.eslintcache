 axios.post(SIGNUP_URL,\r\n                JSON.stringify({ user, pwd, name, email }),\r\n                {\r\n                    headers: { 'Content-Type': 'application/x-www-form-urlencoded',\r\n                    \"Access-Control-Allow-Origin\" : \"localhost:8080\",\r\n                    \"Access-Control-Allow-Methods\" : \"DELETE, POST, GET, OPTIONS\",\r\n                    \"Access-Control-Allow-Headers\" : \"Content-Type, Authorization, X-Requested-With\",\r\n                    'Access-Control-Allow-Credentials' : \"true\"}\r\n                    .withCredentials = true\r\n                }\r\n            );\r\n            console.log(JSON.stringify(response?.data));\r\n            setUser('');\r\n            setPwd('');\r\n            setEmail('');\r\n            setName('');\r\n            setSuccess(true);\r\n        } catch (err) {\r\n            if (!err?.response) {\r\n                setErrMsg('No hay respuesta del servidor');\r\n            } else if (err.response?.status === 400) {\r\n                setErrMsg('Falta un elemento');\r\n            } else {\r\n                setErrMsg('Registro fallido');\r\n            }\r\n            errRef.current.focus();\r\n        }\r\n    }\r\n\r\n    return (\r\n            <>\r\n            {success ? (\r\n                <section>\r\n                    <h1>Se te ha registrado!</h1>\r\n                    <br />\r\n                    <p>\r\n                        <a href=\"/login\">Inicia sesión</a>\r\n                    </p>\r\n                </section>\r\n            ): (\r\n                <div className=\"login-view\">\r\n                <p ref={errRef} className={errMsg ? \"errmsg\" : \"offscreen\"} aria-live=\"assertive\">{errMsg}</p>\r\n                <div className=\"login-view-container\">\r\n                    <div className=\"login-view-header d-flex justify-content-center\">\r\n                        <h1>Registro</h1>\r\n                    </div>\r\n                    <div className=\"login-view-body d-flex justify-content-center\">\r\n                        <form onSubmit={handleSubmit}>\r\n                        <div className=\"form-group p-3\">\r\n                                <label htmlFor=\"name\">Nombre</label>\r\n                                <input \r\n                                type=\"text\" \r\n                                className=\"form-control\" \r\n                                id=\"name\" \r\n                                aria-describedby=\"nameHelp\" \r\n                                placeholder=\"Enter name\"\r\n                                ref={userRef}\r\n                                autoComplete=\"off\"\r\n                                onChange={(e) => setName(e.target.value)}\r\n                                value={name}\r\n                                required />\r\n                            </div>\r\n                            <div className=\"form-group p-3\">\r\n                                <label htmlFor=\"username\">Nombre de usuario</label>\r\n                                <input \r\n                                type=\"text\" \r\n                                className=\"form-control\" \r\n                                id=\"username\" \r\n                                aria-describedby=\"usernameHelp\" \r\n                                placeholder=\"Enter username\"\r\n                                ref={userRef}\r\n                                autoComplete=\"off\"\r\n                                onChange={(e) => setUser(e.target.value)}\r\n                                value={user}\r\n                                required />\r\n                                <small id=\"usernameHelp\" className=\"form-text text-muted\">No se compartirá tu nombre de usuario.</small>\r\n                            </div>\r\n                            <div className=\"form-group\">\r\n                                <label htmlFor=\"password\">Contraseña</label>\r\n                                <input \r\n                                type=\"password\" \r\n                                className=\"form-control\" \r\n                                id=\"password\" \r\n                                placeholder=\"Password\"\r\n                                onChange={(e) => setPwd(e.target.value)}\r\n                                value={pwd}\r\n                                required />\r\n                            </div>\r\n                            <div className=\"form-group p-3\">\r\n                                <label htmlFor=\"email\">Correo Electrónico</label>\r\n                                <input \r\n                                type=\"email\" \r\n                                className=\"form-control\" \r\n                                id=\"email\" \r\n                                aria-describedby=\"emailHelp\" \r\n                                placeholder=\"Enter email\"\r\n                                autoComplete=\"off\"\r\n                                onChange={(e) => setEmail(e.target.value)}\r\n                                value={email}\r\n                                required />\r\n                            </div>\r\n                            <button type=\"submit\" className=\"btn btn-primary\">Registrarme</button>\r\n                        </form>\r\n\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n                )\r\n            }\r\n            \r\n            </>\r\n            \r\n        );\r\n    }\r\n\r\nexport default SignUpView;","C:\\Users\\Fer\\Documents\\THE END\\Proyecto-Final\\tcg-market\\src\\Components\\CardsComponent\\CardInfoComponent\\index.js",[],[],"C:\\Users\\Fer\\Documents\\THE END\\Proyecto-Final\\tcg-market\\src\\Components\\CardsComponent\\CardInfoComponent\\CardInfo.js",[],[],"C:\\Users\\Fer\\Documents\\THE END\\Proyecto-Final\\tcg-market\\src\\Components\\Home\\index.js",[],[],"C:\\Users\\F